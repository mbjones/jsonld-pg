{
  "description": "Load in JSON-LD Playground",
  "manifest_version": 2,
  "name": "jsonld-pg",
  "version": "1.0",

  "background": {
    "scripts": ["background.js"]
  },

  "permissions": [
    "activeTab",
    "tabs"
  ],

  "browser_action": {
    "default_icon": {
      "16": "icons/json-ld-data-16.png",
      "32": "icons/json-ld-data-32.png"
    }
  }
}
